<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/rl_push"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_50sdp"
            android:background="@color/colorf7f7f7"
            android:gravity="center">

            <TextView
                android:id="@+id/txt_notification_setting"
                style="@style/TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:drawableLeft="@mipmap/ico_notification"
                android:drawableRight="@drawable/gray_arrow"
                android:drawablePadding="@dimen/_10sdp"
                android:gravity="center_vertical"
                android:layout_margin="@dimen/_15sdp"
                android:text="@string/txt_notification_setting"
                android:textColor="@color/colordarkGray"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imgArrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/_15sdp"
                android:visibility="gone"
                app:srcCompat="@drawable/gray_arrow" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colordddddd" />

        <RelativeLayout
            android:id="@+id/rl_reject"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorf7f7f7"
            android:gravity="center">

            <TextView
                android:id="@+id/txt_reject"
                style="@style/TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:drawableLeft="@mipmap/ico_reject"
                android:drawablePadding="@dimen/_10sdp"
                android:drawableRight="@drawable/gray_arrow"
                android:gravity="center_vertical"
                android:layout_margin="@dimen/_15sdp"
                android:text="@string/txt_reject_list"
                android:textColor="@color/colordarkGray"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imgArrow1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:visibility="gone"
                android:layout_margin="@dimen/_15sdp"
                app:srcCompat="@drawable/ic_chevron_right_red" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colordddddd" />

        <RelativeLayout
            android:id="@+id/rl_setting"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorf7f7f7">

            <TextView
                android:id="@+id/txt_privacy_setting"
                style="@style/TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:drawableRight="@drawable/gray_arrow"
                android:drawableLeft="@mipmap/ico_privacy"
                android:drawablePadding="@dimen/_10sdp"
                android:text="@string/txt_privacy_settings"
                android:textColor="@color/colordarkGray"
                android:layout_margin="@dimen/_15sdp"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imgArrow2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/_15sdp"
                android:visibility="gone"
                app:srcCompat="@drawable/ic_chevron_right_red" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colordddddd" />

        <RelativeLayout
            android:id="@+id/rl_payment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorf7f7f7">

            <TextView
                android:id="@+id/txt_payment"
                style="@style/TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:drawableLeft="@mipmap/ico_payment"
                android:drawablePadding="@dimen/_10sdp"
                android:drawableRight="@drawable/gray_arrow"
                android:text="Payments"
                android:layout_margin="@dimen/_15sdp"
                android:textColor="@color/colordarkGray"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imgArrow3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/_15sdp"
                android:visibility="gone"
                app:srcCompat="@drawable/ic_chevron_right_red" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colordddddd" />


        <RelativeLayout
            android:id="@+id/rl_help"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorf7f7f7">

            <TextView
                android:id="@+id/txt_help"
                style="@style/TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:drawableLeft="@mipmap/ico_help"
                android:drawablePadding="@dimen/_10sdp"
                android:text="@string/txt_help"
                android:drawableRight="@drawable/gray_arrow"
                android:layout_margin="@dimen/_15sdp"
                android:textColor="@color/colordarkGray"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imgArrow4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/_15sdp"
                android:visibility="gone"
                app:srcCompat="@drawable/ic_chevron_right_red" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colordddddd" />


        <RelativeLayout
            android:id="@+id/rl_password"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorf7f7f7">

            <TextView
                android:id="@+id/txt_password"
                style="@style/TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:drawableLeft="@mipmap/ico_password"
                android:drawablePadding="@dimen/_10sdp"
                android:text="@string/txt_change_pass"
                android:layout_margin="@dimen/_15sdp"
                android:drawableRight="@drawable/gray_arrow"
                android:textColor="@color/colordarkGray"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imgArrow5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/_15sdp"
                android:visibility="gone"
                app:srcCompat="@drawable/ic_chevron_right_red" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colordddddd" />

        <RelativeLayout
            android:id="@+id/about_us_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorf7f7f7">

            <TextView
                android:id="@+id/txt_about_us"
                style="@style/TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:drawableLeft="@mipmap/ico_help"
                android:drawablePadding="@dimen/_10sdp"
                android:drawableRight="@drawable/gray_arrow"
                android:layout_margin="@dimen/_15sdp"
                android:text="@string/txt_about_us"
                android:textColor="@color/colordarkGray"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imgArrow6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/_15sdp"
                android:visibility="gone"
                app:srcCompat="@drawable/ic_chevron_right_red" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colordddddd" />

        <RelativeLayout
            android:id="@+id/live_support_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorf7f7f7">

            <ImageView
                android:id="@+id/img_live_support"
                android:layout_width="@dimen/_22sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/ico_live_support"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/_15sdp"/>

            <TextView
                android:id="@+id/txt_live_support"
                style="@style/TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/_10sdp"
                android:drawableRight="@drawable/gray_arrow"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginBottom="@dimen/_15sdp"
                android:layout_marginRight="@dimen/_15sdp"
                android:layout_toRightOf="@+id/img_live_support"
                android:text="Live Support"
                android:textColor="@color/colordarkGray"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imgArrow7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/_15sdp"
                android:visibility="gone"
                app:srcCompat="@drawable/ic_chevron_right_red" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colordddddd" />

        <RelativeLayout
            android:id="@+id/rl_Logout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorf7f7f7">

            <TextView
                android:id="@+id/txt_logout"
                style="@style/TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:textColor="@color/colordarkGray"
                android:drawableLeft="@mipmap/ico_logout"
                android:drawableRight="@drawable/gray_arrow"
                android:layout_margin="@dimen/_15sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:text="@string/txt_logout"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/_15sdp"
                android:visibility="gone"
                app:srcCompat="@drawable/ic_chevron_right_red" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colordddddd" />
    </LinearLayout>

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:visibility="gone"
        tools:visibility="visible" />

</RelativeLayout>
