<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:padding="10dp"
            android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#64d6b2"
                android:dashGap="0dp"
                android:dashWidth="0dp" />
            <corners
                android:radius="10dp"/>
